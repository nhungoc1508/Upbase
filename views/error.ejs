<% layout('layouts/boilerplate') %>
    <div class="row">
        <div class="alert alert-danger col-6 offset-3" role="alert">
            <h4 class="alert-heading"><%= message %> </h4>
            <% if (process.env.NODE_ENV !== "production") { %> 
            <p><%= err.stack %> </p>
            <% } %> 
            <hr>
            <p class="mb-0"><a href="/">Back to index</a></p>
        </div>
    </div>